.carousel
    .carousel__wrapper
        div(data-bs-ride="carousel" class="carousel slide" id="carouselExampleIndicators")
            .carousel-indicators
                button(type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1")
                button(type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="1" aria-label="Slide 2")
                button(type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="2" aria-label="Slide 3")
                button(type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="3" aria-label="Slide 4")
                button(type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="4" aria-label="Slide 5")
            .carousel-inner
                div(class="carousel-item active")
                    img(src="./assets/img/subnavbar/slider01.png" class="d-block w-100" alt="slide01")
                div(class="carousel-item")
                    img(src="./assets/img/subnavbar/slider02.png" class="d-block w-100" alt="slide02")
                div(class="carousel-item")
                    img(src="./assets/img/subnavbar/slider03.png" class="d-block w-100" alt="slide03")
                div(class="carousel-item")
                    img(src="./assets/img/subnavbar/slider04.png" class="d-block w-100" alt="slide04")
                div(class="carousel-item")
                    img(src="./assets/img/subnavbar/slider05.png" class="d-block w-100" alt="slide05")